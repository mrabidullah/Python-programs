import string



pas=input('Enter short 4 character password  : ')
pass_attack=string.ascii_letters
attempts=0


for i in pass_attack:
    for i1 in pass_attack:
        for i2 in pass_attack:
            for i3 in pass_attack:
                    check=i+i1+i2+i3
                    attempts+=1
                    if check==pas:
                        print(f'password is {check} and it took {attempts} attempts to find it')
                        break
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    

                    
