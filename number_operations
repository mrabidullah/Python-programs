    n=int(input('Enter any number : '))

print('\n even Number ')
for i in range(1,n+1):
    if i % 2 == 0:
        print('even number = ',i)
        
        
print('\n odd number  ')
for i in range(1,n+1):
    if i % 2 !=0:
        print('this is odd number =',i)
        
total=0
for i in range(1,n+1):
    total+=i
    print('sum =',total)
    
print('\n Table ')
for i in range(1,n):

    print(n,'X',i,'=',n*i)
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
